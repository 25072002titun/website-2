{
  "name": "syncano.com",
  "description": "Syncano website",
  "version": "1.0.0",
  "author": "Patryk Kopycinski <patryk.kopycinski@syncano.com>",
  "dependencies": {
    "autolinker": "0.27.0",
    "axios": "0.12.0",
    "babel-cli": "6.18.0",
    "classnames": "2.2.5",
    "formsy-react": "0.18.1",
    "fs": "0.0.2",
    "fs-extra": "0.30.0",
    "gatsby": "0.12.18",
    "hellojs": "1.14.0",
    "highlight.js": "9.7.0",
    "js-cookie": "2.1.3",
    "js-yaml": "3.6.1",
    "lodash": "4.16.4",
    "moment": "2.15.2",
    "normalize.css": "4.2.0",
    "numeral": "1.5.3",
    "react-body-classname": "1.1.0",
    "react-headroom": "2.1.3",
    "react-helmet": "3.1.0",
    "react-modal": "1.5.2",
    "react-mousetrap": "0.1.1",
    "react-prism": "4.0.0",
    "react-scroll": "1.4.4",
    "react-slick": "0.14.5",
    "react-sticky": "5.0.5",
    "sitemap": "1.8.2",
    "syncano": "1.0.21",
    "twit": "2.2.5"
  },
  "devDependencies": {
    "eslint": "2.13.1",
    "eslint-config-airbnb": "9.0.1",
    "eslint-plugin-import": "1.10.2",
    "eslint-plugin-jsx-a11y": "1.5.3",
    "eslint-plugin-react": "5.2.2",
    "extended-define-webpack-plugin": "0.1.2",
    "nightwatch": "0.9.1",
    "selenium-standalone": "5.1.0"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "main": "n/a",
  "scripts": {
    "get-pricing-plans": "babel-node ./scripts/get-pricing-plans.js",
    "get-tweets": "babel-node ./scripts/get-tweets.js",
    "setup-data": "npm run get-pricing-plans && npm run get-tweets",
    "prestart": "npm run setup-data",
    "start": "gatsby develop",
    "prebuild": "npm run setup-data",
    "build": "NODE_ENV=staging gatsby build",
    "lint": "./node_modules/.bin/eslint --ext .js,.jsx --ignore-pattern public .",
    "test": "echo \"Error: no test specified\" && exit 1",
    "e2e-setup": "mkdir -p reports/ && selenium-standalone install --drivers.chrome.version=2.16",
    "e2e-additional-mac-setup": "./configureMacE2E.sh",
    "e2e-http-server": "cd public/ && nohup python -m SimpleHTTPServer &",
    "e2e-chrome": "nightwatch -e chrome --suiteRetries 1",
    "e2e-safari": "nightwatch -e safari --suiteRetries 1",
    "e2e-firefox": "nightwatch -e firefox --suiteRetries 1",
    "e2e-local": "nightwatch -e chrome,firefox --suiteRetries 1"
  }
}
